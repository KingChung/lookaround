{
    "manifest_version": 2,

    "name": "Look Around",
    "description": "All is for engineer's health",
    "version": "1.0",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy":"script-src 'self' https://www.google.com; object-src 'self'",
    "browser_action": {
        "default_icon": "icon.png",
    },
    "permissions": [
        "activeTab"
    ]
}
